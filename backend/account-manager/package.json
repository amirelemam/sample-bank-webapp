{
  "name": "account-manager",
  "version": "1.0.0",
  "description": "This service allows you to manage your bank account",
  "main": "index.js",
  "scripts": {
    "dev": "cd src && NODE_ENV=dev nodemon -r dotenv/config index.js",
    "prestart": "node -e \"require('./src/db/createTables/controller.js')();\"",
    "start": "cd src && node index.js",
    "test": "NODE_ENV=test jest --detectOpenHandles --runInBand --coverage",
    "test:unit": "NODE_ENV=test jest src/components/**/__tests__/unit/**/*.test.js --detectOpenHandles --runInBand --coverage",
    "test:integration": "NODE_ENV=test jest src/components/**/__tests__/integration/*.test.js --detectOpenHandles --runInBand --coverage"
  },
  "keywords": [],
  "author": "Amir Elemam",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.4",
    "knex": "^0.21.17",
    "morgan": "^1.10.0",
    "pg": "^8.5.1",
    "sanitize": "^2.1.0",
    "swagger-jsdoc": "^6.0.2",
    "swagger-ui-express": "^4.1.6",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.3"
  }
}
