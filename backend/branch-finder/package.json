{
  "name": "branch-finder",
  "version": "1.0.0",
  "description": "This service allows you to find the nearest branch",
  "scripts": {
    "build": "tsc",
    "prestart": "npm run build && node dist/db/populate/services.js checkIfCollectionIsEmptyAndPopulate",
    "start": "node dist/app",
    "start:local": "NODE_ENV=dev nodemon src/index.ts",
    "test": "NODE_ENV=test jest --detectOpenHandles --runInBand --coverage",
    "test:unit": "NODE_ENV=test jest src/components/**/__tests__/unit/**/*.test.js --detectOpenHandles --runInBand --coverage",
    "test:integration": "NODE_ENV=test jest src/components/**/__tests__/integration/*.test.js --detectOpenHandles --runInBand --coverage"
  },
  "keywords": [],
  "author": "Amir Elemam",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/mongodb": "^3.6.3",
    "@types/mongoose": "5.10.1",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.25",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.3.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.5.0",
    "logform": "^2.2.0",
    "mongoose": "5.10.1",
    "morgan": "^1.10.0",
    "swagger-ui-express": "^4.1.6",
    "typescript": "^3.9.7",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.0"
  }
}